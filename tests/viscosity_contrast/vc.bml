        <include_in_statistics/>
      </diagnostics>
    </coefficient>
    <nonlinear_solver name="Solver">
      <type name="Picard">
        <preamble>
          <string_value lines="20" type="code" language="python">rv = inner(sym(grad(v_t)), 2.e4*sym(grad(v_a)))*dx(1) \
  + inner(sym(grad(v_t)), 2.*sym(grad(v_a)))*dx(2) \
  - inner(v_t, s_i)*dx(1) - inner(v_t, s_i)*dx(2)

r = rv</string_value>
        </preamble>
        <form name="Bilinear" rank="1">
          <string_value lines="20" type="code" language="python">a = lhs(r)</string_value>
          <ufl_symbol>
