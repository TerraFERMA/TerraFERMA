<?xml version='1.0' encoding='utf-8'?>
<terraferma_options>
  <geometry>
    <dimension>
      <integer_value rank="0">2</integer_value>
    </dimension>
    <mesh name="Mesh">
      <source name="File">
        <file>
          <string_value lines="1" type="filename">region_id</string_value>
        </file>
        <cell>
          <string_value lines="1">triangle</string_value>
        </cell>
      </source>
    </mesh>
  </geometry>
  <io>
    <output_base_name>
      <string_value lines="1">region_ids</string_value>
    </output_base_name>
    <dump_periods/>
    <detectors>
      <point name="Lower">
        <real_value shape="2" dim1="dim" rank="1">0.25 0.225</real_value>
      </point>
      <point name="LowerMiddle">
        <real_value shape="2" dim1="dim" rank="1">0.25 0.475</real_value>
      </point>
      <point name="UpperMiddle">
        <real_value shape="2" dim1="dim" rank="1">0.25 0.525</real_value>
      </point>
      <point name="Upper">
        <real_value shape="2" dim1="dim" rank="1">0.25 0.775</real_value>
      </point>
    </detectors>
  </io>
  <global_parameters/>
  <system name="Regions">
    <mesh name="Mesh"/>
    <ufl_symbol name="global">
      <string_value lines="1">ur</string_value>
    </ufl_symbol>
    <field name="Field">
      <ufl_symbol name="global">
        <string_value lines="1">f</string_value>
      </ufl_symbol>
      <type name="Function">
        <rank name="Scalar" rank="0">
          <element name="UserDefined">
            <family>
              <string_value lines="1">DG</string_value>
            </family>
            <degree>
              <integer_value rank="0">1</integer_value>
            </degree>
          </element>
          <initial_condition type="initial_condition" name="Lower">
            <region_ids>
              <integer_value shape="1" rank="1">35</integer_value>
            </region_ids>
            <constant>
              <real_value rank="0">35</real_value>
            </constant>
          </initial_condition>
          <initial_condition type="initial_condition" name="LowerMiddle">
            <region_ids>
              <integer_value shape="1" rank="1">34</integer_value>
            </region_ids>
            <constant>
              <real_value rank="0">34</real_value>
            </constant>
          </initial_condition>
          <initial_condition type="initial_condition" name="UpperMiddle">
            <region_ids>
              <integer_value shape="1" rank="1">33</integer_value>
            </region_ids>
            <constant>
              <real_value rank="0">33</real_value>
            </constant>
          </initial_condition>
          <initial_condition type="initial_condition" name="Upper">
            <region_ids>
              <integer_value shape="1" rank="1">32</integer_value>
            </region_ids>
            <constant>
              <real_value rank="0">32</real_value>
            </constant>
          </initial_condition>
        </rank>
      </type>
      <diagnostics>
        <include_in_visualization/>
        <include_in_statistics/>
        <include_in_detectors/>
      </diagnostics>
    </field>
    <coefficient name="Coefficient">
      <ufl_symbol name="global">
        <string_value lines="1">c</string_value>
      </ufl_symbol>
      <type name="Expression">
        <rank name="Scalar" rank="0">
          <element name="UserDefined">
            <family>
              <string_value lines="1">DG</string_value>
            </family>
            <degree>
              <integer_value rank="0">1</integer_value>
            </degree>
          </element>
          <value type="value" name="Lower">
            <region_ids>
              <integer_value shape="1" rank="1">35</integer_value>
            </region_ids>
            <constant>
              <real_value rank="0">35</real_value>
            </constant>
          </value>
          <value type="value" name="LowerMiddle">
            <region_ids>
              <integer_value shape="1" rank="1">34</integer_value>
            </region_ids>
            <constant>
              <real_value rank="0">34</real_value>
            </constant>
          </value>
          <value type="value" name="UpperMiddle">
            <region_ids>
              <integer_value shape="1" rank="1">33</integer_value>
            </region_ids>
            <constant>
              <real_value rank="0">33</real_value>
            </constant>
          </value>
          <value type="value" name="Upper">
            <region_ids>
              <integer_value shape="1" rank="1">32</integer_value>
            </region_ids>
            <constant>
              <real_value rank="0">32</real_value>
            </constant>
          </value>
        </rank>
      </type>
      <diagnostics>
        <include_in_statistics/>
        <include_in_detectors/>
      </diagnostics>
    </coefficient>
    <nonlinear_solver name="Solver">
      <type name="SNES">
        <form name="Residual" rank="0">
          <string_value lines="20" type="code" language="python">r = f_t*(f_i-c_i)*dx(32) + f_t*(f_i-c_i)*dx(33) + f_t*(f_i-c_i)*dx(34) + f_t*(f_i-c_i)*dx(35)</string_value>
          <ufl_symbol name="solver">
            <string_value lines="1">r</string_value>
          </ufl_symbol>
        </form>
        <form name="Jacobian" rank="1">
          <string_value lines="20" type="code" language="python">a = derivative(r, ur_i, ur_a)</string_value>
          <ufl_symbol name="solver">
            <string_value lines="1">a</string_value>
          </ufl_symbol>
        </form>
        <quadrature_rule name="default"/>
        <snes_type name="ls">
          <ls_type name="cubic"/>
          <convergence_test name="default"/>
        </snes_type>
        <relative_error>
          <real_value rank="0">1.e-6</real_value>
        </relative_error>
        <absolute_error>
          <real_value rank="0">1.e-16</real_value>
        </absolute_error>
        <max_iterations>
          <integer_value rank="0">5</integer_value>
        </max_iterations>
        <monitors>
          <residual/>
        </monitors>
        <linear_solver>
          <iterative_method name="cg">
            <relative_error>
              <real_value rank="0">1.e-8</real_value>
            </relative_error>
            <max_iterations>
              <integer_value rank="0">100</integer_value>
            </max_iterations>
            <zero_initial_guess/>
            <monitors>
              <preconditioned_residual/>
            </monitors>
          </iterative_method>
          <preconditioner name="sor"/>
        </linear_solver>
        <never_ignore_solver_failures/>
      </type>
      <solve name="in_timeloop"/>
    </nonlinear_solver>
  </system>
</terraferma_options>
